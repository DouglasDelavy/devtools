{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES2018",
    "moduleResolution": "Node",
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": ["@types/node", "@citizenfx/client", "../shared/types/"],
    "paths": {
      "@shared/*": ["../shared/*"],
      "@modules/*": ["./modules/*"],
      "@utils/*": ["./utils/*"]
    }
  },
  "include": ["./"]
}
